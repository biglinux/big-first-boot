#!/bin/bash

if [ ! -e "/rofs" ]; then

    
    #Configure timeshift
    if [ "$(grep btrfs /etc/fstab)" != "" ]; then
        timeshift --btrfs
        sed -i 's|"schedule_daily" : "false"|"schedule_daily" : "true"|g;s|"count_daily" : "5"|"count_daily" : "7"|g' /etc/timeshift.json
        timeshift --check
    fi
    
    # Fix to nvidia prime
    prime-select on-demand
    
#Disable first boot
dpkg -P big-first-boot
    
fi


